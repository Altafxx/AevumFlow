{
  "name": "aevumflow",
  "version": "2.3.1",
  "private": true,
  "scripts": {
    "dev": "ts-node scripts/generate-auth-secret.ts && next dev --turbopack",
    "build": "ts-node scripts/generate-auth-secret.ts && next build",
    "start": "ts-node scripts/generate-auth-secret.ts && next start",
    "lint": "next lint",
    "migrate": "npx prisma migrate dev",
    "docker:up": "./detect-arch.sh up",
    "docker:build": "./detect-arch.sh build",
    "docker:up:arm": "TARGETPLATFORM=linux/arm64 ./detect-arch.sh up",
    "docker:up:amd": "TARGETPLATFORM=linux/amd64 ./detect-arch.sh up",
    "docker:down": "docker compose down",
    "docker:test:arm": "docker run --rm --privileged multiarch/qemu-user-static --reset -p yes && npm run docker:up:arm",
    "docker:inspect": "docker inspect $(docker ps -q) | grep Architecture",
    "generate-secret": "ts-node scripts/generate-auth-secret.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@hookform/resolvers": "^4.1.0",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "fluent-ffmpeg": "^2.1.3",
    "hls.js": "^1.5.20",
    "hono": "^4.7.0",
    "lucide-react": "^0.475.0",
    "minio": "^8.0.4",
    "next": "^15.2.4",
    "next-auth": "^4.24.7",
    "next-themes": "^0.4.6",
    "plyr": "^3.7.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.54.2",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcrypt": "^5.0.2",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}